        </Button>
        <Button Style="{DynamicResource ButtonStyle2}" x:Name="fo_button" Content="" HorizontalAlignment="Left" Margin="9,6,0,0" VerticalAlignment="Top" Height="126" Width="197" BorderBrush="{x:Null}" Click="fo_button_Click">
