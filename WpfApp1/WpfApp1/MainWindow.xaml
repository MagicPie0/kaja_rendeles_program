    <Grid x:Name="fooldal" Height="1000" Width="1700">
        <Grid.Background>
            <ImageBrush ImageSource="/images/512873.jpg" Stretch="UniformToFill"/>
        </Grid.Background>
        <Rectangle HorizontalAlignment="Center" Height="134" VerticalAlignment="Top" Width="1700" Fill="White" Opacity="0.7"/>
        <Label Content="Your one-stop Eatopia for delicious food." HorizontalAlignment="Center" VerticalAlignment="Top" Height="57" Width="660" FontSize="36" Margin="0,17,0,0" FontStyle="Italic" FontFamily="Modern No. 20"/>

